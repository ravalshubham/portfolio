<!DOCTYPE html>
<html lang="en">
<%- include('partials/head') %>
<head>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
  <%- include('partials/nav') %>

  <section>
    <h2>Projects</h2>
    <% data.projects.forEach(project => { %>
      <div class="project-card">
        <h3><%= project.title %></h3>
        <p><%= project.description %></p>
      </div>
    <% }) %>
  </section>

  <%- include('partials/footer') %>
</body>
</html>
